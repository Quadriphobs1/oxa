{
    "name": "oxa: codespace",
    "extensions": [
        "cschleiden.vscode-github-actions",
        "matklad.rust-analyzer",
        "serayuzgur.crates",
        "vadimcn.vscode-lldb"
    ],
    "dockerFile": "Dockerfile",
    "settings": {
        "editor.formatOnSave": true,
        "terminal.integrated.shell.linux": "/usr/bin/zsh",
        "files.exclude": {
            "**/CODE_OF_CONDUCT.md": true,
            "**/LICENSE": true,
            "**/debug.log": true
        }
    }
}