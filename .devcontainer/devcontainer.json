{
    "name": "oxa: codespace",
    "extensions": [
        "cschleiden.vscode-github-actions",
        "serayuzgur.crates",
        "vadimcn.vscode-lldb",
        "bungcip.better-toml",
        "ms-azuretools.vscode-docker",
        "rust-lang.rust-analyzer"
    ],
    "dockerFile": "Dockerfile",
    "settings": {
        "editor.formatOnSave": true,
        "terminal.integrated.shell.linux": "/usr/bin/zsh",
        "files.exclude": {
            "**/CODE_OF_CONDUCT.md": true,
            "**/LICENSE": true,
            "**/debug.log": true
        }
    }
}